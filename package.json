    "nodes": [
      "dist/nodes/ChuckyJob/ChuckyJob.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.1.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@chucky.cloud/sdk": "^0.3.0"
